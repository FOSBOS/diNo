<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
      <section name="log4net"
          type="log4net.Config.Log4NetConfigurationSectionHandler, log4net, Version=1.2.13.0, Culture=neutral, PublicKeyToken=669e0ddf0bb1aa2a"
      />
    </configSections>
    <connectionStrings>
        <add name="diNo.Properties.Settings.diNoConnectionString" connectionString="Data Source=192.168.235.3,1433;Network Library=DBMSSOCN;Initial Catalog=diNo;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
  <log4net debug="true">
    <root>
    <level value="ALL"/>
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file value="log.txt" />
      <appendToFile value="true" />
      <layout type="log4net.Layout.PatternLayout">
